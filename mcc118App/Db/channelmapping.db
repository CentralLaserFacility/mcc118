record("calcout", "$(P):$(R):GUN"){
	field("DESC", "connected to CH1")
	field("SCAN", ".1 second")
	field("INPA", "$(P):$(R):HA0:CH1 PP")
	field("CALC", "A*49.5 - 61.3")
	field("EGU", "dBm")
	field("LOPR", "-30")
	field("HOPR", "10")
}

