record("calcout", "$(P):$(R):VOLT:5VP"){
        field("DESC", "connected to HAT1 CH0")
        field("SCAN", ".1 second")
        field("INPA", "$(P):$(R):HA0:CH0 PP")
        field("CALC", "A")
        field("EGU", "V")
        field("LOPR", "0")
        field("HOPR", "20")
}

